{
  "name": "svelte-memorii",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --host",
    "postbuild": "node scripts/bump-sw-version.js",
    "preview": "vite preview --host",
    "start": "wrangler dev",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "npm run build && wrangler deploy",
    "postdeploy": "node scripts/tag-deploy.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "@tsconfig/svelte": "^2.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^4.5.2",
    "wrangler": "4.54.0"
  }
}
