{
  "name": "svelte-memorii",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --host",
    "postbuild": "node scripts/bump-sw-version.js",
    "preview": "vite preview --host",
    "start": "wrangler dev",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "npm run build && wrangler deploy",
    "postdeploy": "node scripts/tag-deploy.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "wrangler": "4.54.0"
  }
}
